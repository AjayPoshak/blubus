{
  "name": "blubus",
  "version": "1.0.0",
  "description": "bus booking in africa",
  "main": "index.js",
  "scripts": {
    "dev:web": "webpack-dev-server --mode development --config ./webpack.client.js",
    "build": "yarn build:web && yarn build:ssr",
    "build:web": "yarn run clean && webpack --mode production --config ./webpack.client.js",
    "build:ssr": "webpack --mode production --config ./webpack.server.js",
    "dev:api": "",
    "dev:ssr": "webpack --mode development --config ./webpack.server.js --watch & yarn start",
    "start": "node build/server/main.bundle.js",
    "clean": "rm -rf build",
    "build:stats": "webpack --env production --json > stats.json",
    "test": "jest --config ./jest.config.json",
    "test:watch": "jest --config ./jest.config.json --watch",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --config ./jest.config.json --runInBand --watch",
    "test:debug:win": "node --inspect-brk ./node_modules/jest/bin/jest.js --config ./jest.config.json --runInBand --watch",
    "test:snapshot": "jest --config ./jest.config.json -u",
    "flow start": "flow start",
    "flow stop": "flow stop",
    "flow status": "flow status",
    "flow coverage": "flow coverage"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": "9.x"
  },
  "author": "Ajay Poshak",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "assets-webpack-plugin": "^3.8.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-styled-components": "^1.5.1",
    "brotli-webpack-plugin": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.2.0",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.1",
    "raven": "^2.6.3",
    "rc-calendar": "^9.6.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.3.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.76.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "react-test-renderer": "^16.4.1",
    "webpack-dev-server": "^3.1.4"
  }
}
