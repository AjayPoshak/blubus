{
	"env": {
		"development": {
			"presets": [
				[
					"env",
					{
						"targets": {
							"browsers": ["last 2 versions"],
							"node": "current"
						},
						"debug": false
					}
				],
				"react",
				"stage-2"
			],
			"plugins": [
				["babel-plugin-styled-components", {
					"ssr": true
				  }]
			]
		},
		"production": {
			"presets": [
				[
					"env",
					{
						"targets": {
							"browsers": ["last 2 versions"],
							"node": "current"
						},
						"debug": false
					}
				],
				"react",
				"stage-2"
			],
			"plugins": [
				["babel-plugin-styled-components", {
					"ssr": true
				  }]
			]
		},
		"test": {
			"presets": [
				[
					"env",
					{"modules": "commonjs"}
				],
				"react",
				"stage-2"
			]
		}
	}
}